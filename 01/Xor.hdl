/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=t01);
    Or (a=a, b=b, out=t02);
    And(a=t01, b=t02, out=out);
}